{
    "control":[
        { "VTOY_DEFAULT_SEARCH_ROOT": "/ISO" }
    ],
    "auto_install":[
        {
            "parent": "/ISO/Windows 11",
            "template":[
                "/ventoy/script/_win_11_bypass_NRO.xml",
                "/ventoy/script/_win_11_full_auto.xml",
                "/ventoy/script/_win_11_full_auto_with_username.xml"
            ],
            "timeout": 0,
            "autosel": 1
        },
        {
            "parent": "/ISO/Windows 10",
            "template":[
                "/ventoy/script/_win_10_full_auto.xml",
                "/ventoy/script/_win_10_full_auto_with_username.xml"
            ],
            "timeout": 0,
            "autosel": 0
        },
        {
            "parent": "/ISO/Windows 8",
            "template":[
                "/ventoy/script/_win_8_bypass_product_key.xml",
                "/ventoy/script/_win_8_full_auto.xml",
                "/ventoy/script/_win_8_full_auto_with_username.xml"
            ],
            "timeout": 0,
            "autosel": 1
        },
        {
            "parent": "/ISO/Windows 7",
            "template":[
                "/ventoy/script/_win_7_full_auto.xml",
                "/ventoy/script/_win_7_full_auto_with_username.xml"
            ],
            "timeout": 0,
            "autosel": 0
        }
    ]
